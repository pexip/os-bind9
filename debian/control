Package: libisccfg163
Section: libs
Architecture: any
Multi-Arch: same
Depends: libdns1104 (= ${binary:Version}),
         libisc1100 (= ${binary:Version}),
         libisccc161 (= ${binary:Version}),
         ${misc:Depends},
